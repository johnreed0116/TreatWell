                                    ));
                        }
                        $('#displayMemDiv').html('');
                        $('#displayMemDiv').append($tbl);
                        return false;
                    } else {
                        $('#displayMemDiv').html('');
                        $tbl.append(
                                $('<tr>').append(
                                $('<td class="center aligned negative" colspan="7">').html('<b>No data found.</b>')
                                ));
                        $('#displayMemDiv').append($tbl);
                        return false;
                    }
