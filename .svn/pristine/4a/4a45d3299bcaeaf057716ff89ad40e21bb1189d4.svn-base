                            }
                            $tbl.append(
                                    $('<tr>').append(
                                    $('<td align="center">').html(eval(i + 1)),
                                    $('<td>').html(list[i].TITLE),
                                    $('<td>').html(list[i].DATE_FROM),
                                    $('<td >').html(list[i].DATE_TO)
//                                    $('<td align="center">').html('<i class="fa fa-upload" aria-hidden="true" title="Click to Delete" style="cursor: pointer;" onclick="saveDoctorAttachements(\'' + 'experience' + '\');"></i>'),
//                                    $('<td align="center">').html('<i class="fa fa-paperclip" aria-hidden="true" title="Click to View" style="cursor: pointer;" onclick="displayDoctorAttachements(\'' + 'experience' + '\');"></i>')
//                                    $('<td align="center">').html(editHtm),
//                                    $('<td align="center">').html(delHtm)
                                    ));
                        }
                        $('#displayExpDiv').html('');
                        $('#displayExpDiv').append($tbl);
                        return false;
                    } else {
                        $('#displayExpDiv').html('');
                        $tbl.append(
                                $('<tr>').append(
                                $('<td class="center aligned negative" colspan="7">').html('<b>No data found.</b>')
