                                    $('<td>').html(list[i].TITLE)
//                                    $('<td align="center">').html('<i class="fa fa-upload" aria-hidden="true" title="Click to Delete" style="cursor: pointer;" onclick="saveDoctorAttachements(\'' + 'association' + '\');"></i>'),
//                                    $('<td align="center">').html('<i class="fa fa-paperclip" aria-hidden="true" title="Click to View" style="cursor: pointer;" onclick="displayDoctorAttachements(\'' + 'association' + '\');"></i>')
